{
  "name": "dub-laravel-migration",
  "version": "1.0.0",
  "description": "Comprehensive migration of Dub.co architecture from Next.js to Laravel + React + Inertia.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && php artisan serve",
    "dev:frontend": "vite",
    "build": "vite build",
    "test": "npm run test:backend",
    "test:backend": "cd backend && php artisan test",
    "lint": "npm run lint:backend",
    "lint:backend": "cd backend && ./vendor/bin/phpcs",
    "setup": "npm run setup:backend",
    "setup:backend": "cd backend && composer install && cp .env.example .env && php artisan key:generate"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "concurrently": "^8.2.2",
    "laravel-vite-plugin": "^2.0.0",
    "vite": "^7.0.6"
  },
  "keywords": [
    "laravel",
    "react",
    "inertiajs",
    "typescript",
    "url-shortener",
    "dub",
    "migration",
    "php",
    "javascript"
  ],
  "author": {
    "name": "Yasin Boelhouwer",
    "email": "yasin@enginebit.com",
    "url": "https://github.com/makafeli"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/makafeli/dub-laravel-migration.git"
  },
  "bugs": {
    "url": "https://github.com/makafeli/dub-laravel-migration/issues"
  },
  "homepage": "https://github.com/makafeli/dub-laravel-migration#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@inertiajs/react": "^2.0.17",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.536.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2"
  }
}
