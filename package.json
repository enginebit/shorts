{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "shorts",
  "version": "1.0.0",
  "description": "Comprehensive migration of Dub.co architecture from Next.js to Laravel + React + Inertia.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"php artisan serve\" \"vite\"",
    "build": "vite build",
    "test": "php artisan test",
    "test:frontend": "vitest",
    "test:frontend:ui": "vitest --ui",
    "test:frontend:run": "vitest run",
    "test:frontend:coverage": "vitest run --coverage",
    "test:frontend:watch": "vitest --watch",
    "test:all": "npm run test:frontend:run && npm run test",
    "lint": "./vendor/bin/phpcs",
    "setup": "composer install && cp .env.example .env && php artisan key:generate && npm install"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^3.2.4",
    "axios": "^1.8.2",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "laravel-vite-plugin": "^2.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "laravel",
    "react",
    "inertiajs",
    "typescript",
    "url-shortener",
    "dub",
    "migration",
    "php",
    "javascript"
  ],
  "author": {
    "name": "Yasin Boelhouwer",
    "email": "yasin@enginebit.com",
    "url": "https://github.com/makafeli"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/makafeli/dub-laravel-migration.git"
  },
  "bugs": {
    "url": "https://github.com/makafeli/dub-laravel-migration/issues"
  },
  "homepage": "https://github.com/makafeli/dub-laravel-migration#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@inertiajs/react": "^2.0.17",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.536.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "slugify": "^1.6.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "ziggy-js": "^2.5.3"
  }
}
