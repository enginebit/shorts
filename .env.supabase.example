# Laravel Environment Configuration for Supabase PostgreSQL
# Copy this file to .env and update with your actual Supabase credentials

APP_NAME="Shorts - URL Shortener"
APP_ENV=local
APP_KEY=base64:your-app-key-here
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost:8000

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# Logging Configuration
LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration - Supabase PostgreSQL
DB_CONNECTION=pgsql
DB_URL=postgresql://postgres.[project-ref]:[db-password]@[host].pooler.supabase.com:5432/postgres

# Individual connection parameters (auto-filled from DB_URL)
DB_HOST=[host].pooler.supabase.com
DB_PORT=5432
DB_DATABASE=postgres
DB_USERNAME=postgres.[project-ref]
DB_PASSWORD=[your-database-password]

# PostgreSQL specific settings
DB_CHARSET=utf8
DB_SCHEMA=public
DB_SSLMODE=require

# Example for project hlipluswsrhzfkjixowk (Bimiup.com):
# DB_URL=postgresql://postgres.hlipluswsrhzfkjixowk:[password]@aws-0-eu-central-1.pooler.supabase.com:5432/postgres
# DB_HOST=aws-0-eu-central-1.pooler.supabase.com
# DB_USERNAME=postgres.hlipluswsrhzfkjixowk

# Session Configuration
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Cache Configuration
CACHE_STORE=database
CACHE_PREFIX=

# Queue Configuration
QUEUE_CONNECTION=database

# Mail Configuration
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Broadcasting
BROADCAST_CONNECTION=log

# Filesystem
FILESYSTEM_DISK=local

